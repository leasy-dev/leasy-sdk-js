{
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "codegen": "graphql-codegen"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "name": "leasy",
  "author": "hendrikniemann",
  "module": "dist/leasy-sdk-js.esm.js",
  "size-limit": [
    {
      "path": "dist/leasy-sdk-js.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/leasy-sdk-js.esm.js",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@size-limit/preset-small-lib": "^4.9.2",
    "dotenv": "^8.2.0",
    "husky": "^5.1.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0"
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
